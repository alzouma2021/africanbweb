            };\n            reader.readAsDataURL(this.selectedFile);\n        },\n\n        // ENREGISTRER LE DOCUMENT\n        async saveDocumentTransport(){\n            const params = {\n                documentName : this.selectedFile,\n                raisonSociale : this.compagnieTransportRaisonSociale\n            }\n            await axios.post(API_CREER_DOCUMENT_TRANSPORT, { params }).then((response) => {\n\n            })\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}