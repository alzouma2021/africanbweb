erenceTypeOffreVoyageList = response.data.items\n            }).catch((e) => {\n                this.errorMsg = e ;\n                $(\".alert-error\").fadeIn();\n                setTimeout(function(){\n                    $(\".alert-error\").fadeOut(); \n                }, 4000)\n            })\n        },\n\n    },\n\n    mounted(){\n        this.obtenirReferenceModeOffreVoyage()\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}