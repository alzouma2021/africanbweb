         localStorage.setItem('isAuthenticated', 'true');\n                localStorage.setItem('userRole', 'adminCompagnieTransport');\n                this.$router.push('/EspaceCompagnieTransport');\n            } else if (this.username === 'user' && this.password === 'user') {\n                localStorage.setItem('isAuthenticated', 'true');\n                localStorage.setItem('userRole', 'user');\n                this.$router.push('/user');\n            } else {\n                alert('Identifiants incorrects');\n            }\n        },\n\n    },\n\n    mounted(){}\n}\n</script>\n\n<style scoped>\n    .title{\n        padding-top: 40px;\n        color: black;\n        font-weight: bold;\n        font-size: 60px;\n    }\n\n    .connexion_text{\n        color: white;\n    }\n\n    .password_forget{\n        color: grey;\n    }\n\n\n    .password_forget:hover{\n        cursor: pointer;\n        color: #487eb0;\n    }\n</style>"]},"metadata":{},"sourceType":"module"}