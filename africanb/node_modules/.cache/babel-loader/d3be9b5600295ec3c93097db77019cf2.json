{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport $ from 'jquery';\n//import { required } from 'vuelidate/lib/validators'\nimport { API_RECUPERER_LISTE_OFFRE_VOYAGE, API_OBTENIR_LISTE_PROPRIETE_OFFRE_VOYAGE } from '../globalConfig/globalConstConfig';\nexport default {\n  name: \"CreerProprieteCarateristiqueOffreVoyage\",\n  data() {\n    return {\n      errorMsg: null,\n      successMsg: null,\n      overlay: null,\n      offreVoyageParCompagnieTransportList: [],\n      proprieteOffreVoyagesList: [],\n      valeurCaracteristiqueOffreList: [],\n      valeurCaracteristiqueObject: {\n        datas: []\n      },\n      valeurCaracteristiqueModel: {},\n      offreVoyageObject: {\n        data: {\n          compagnieTransportRaisonSociale: \"COMPAGNIE KOUEVI CT\"\n        }\n      },\n      simpleObject: {}\n    };\n  },\n  methods: {\n    //RECUPERER LA LISTE DES OFFRES DE VOYAGES PAR COMPAGNIE DE TRANSPORT\n    async obtenirOffreVoyageParCompagnieTransport() {\n      await axios.post(API_RECUPERER_LISTE_OFFRE_VOYAGE, this.offreVoyageObject).then(response => {\n        console.log(response);\n        if (response.status == 200) {\n          if (response.data.status.code != 800) {\n            this.errorMsg = response.data.status.message;\n            $(\".alert-error\").fadeIn();\n            setTimeout(function () {\n              $(\".alert-error\").fadeOut();\n            }, 4000);\n          } else {\n            this.offreVoyageParCompagnieTransportList = response.data.items;\n          }\n        } else {\n          this.errorMsg = \"Erreur\";\n          $(\".alert-error\").fadeIn();\n          setTimeout(function () {\n            $(\".alert-error\").fadeOut();\n          }, 4000);\n        }\n      }).catch(e => {\n        this.errorMsg = e;\n      });\n    },\n    // OBTENIR LA LISTE DES PROPRIETES PORTANT SUR LES OFFRES DE VOYAGES\n    async obtenirProprieteOffreVoyage() {\n      await axios.post(API_OBTENIR_LISTE_PROPRIETE_OFFRE_VOYAGE, this.simpleObject).then(response => {\n        if (response.status == 200) {\n          if (response.data.status.code != 800) {\n            this.errorMsg = response.data.status.message;\n            $(\".alert-error\").fadeIn();\n            setTimeout(function () {\n              $(\".alert-error\").fadeOut();\n            }, 4000);\n          } else {\n            this.proprieteOffreVoyagesList = response.data.items;\n            this.proprieteOffreVoyagesList.forEach(element => {\n              var valeurCaracteristiqueOffre = {\n                valeurTexte: null,\n                offreVoyageDesignation: null,\n                proprieteOffreVoyageDesignation: null\n              };\n              valeurCaracteristiqueOffre.proprieteOffreVoyageDesignation = element.designation;\n              this.valeurCaracteristiqueOffreList.push(valeurCaracteristiqueOffre);\n            });\n          }\n        } else {\n          this.errorMsg = \"Erreur\";\n          $(\".alert-error\").fadeIn();\n          setTimeout(function () {\n            $(\".alert-error\").fadeOut();\n          }, 4000);\n        }\n      });\n    }\n  },\n  mounted() {\n    this.obtenirOffreVoyageParCompagnieTransport();\n    //this.obtenirProprieteOffreVoyage();\n  }\n};","map":{"version":3,"mappings":";AAyCA;AACA;AACA;AACA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MAEAC;MACAC;MACAC;MAEAC;QACAC;MACA;MAEAC,6BAEA;MAEAC;QACAV;UACAW;QACA;MACA;MAEAC;IACA;EACA;EAEAC;IAEA;IACA;MACA;QACAC;QACA;UACA;YACA;YACAC;YACAC;cACAD;YACA;UACA;YACA;UACA;QACA;UACA;UACAA;UACAC;YACAD;UACA;QACA;MACA;QACA;MACA;IACA;IAGA;IACA;MACA;QACA;UACA;YACA;YACAA;YACAC;cACAD;YACA;UACA;YACA;YACA;cACA;gBACAE;gBACAC;gBACAC;cACA;cACAC;cACA;YACA;UACA;QACA;UACA;UACAL;UACAC;YACAD;UACA;QACA;MACA;IACA;EAEA;EAGAM;IACA;IACA;EACA;AACA","names":["name","data","errorMsg","successMsg","overlay","offreVoyageParCompagnieTransportList","proprieteOffreVoyagesList","valeurCaracteristiqueOffreList","valeurCaracteristiqueObject","